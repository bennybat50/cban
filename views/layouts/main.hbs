                            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                        </div>
                        {{/if }}
                        {{#if error}}
                        <div class="alert alert-danger alert-dismissible fade show border-0 b-round" role="alert">
                            <strong>Error</strong> {{ error }}
                            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                        </div>
                        {{/if }}
                    </div>
